{
  "name": "Coderbility",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Add syntax highlighting to Readability",
  "content_scripts": [
    {
      "matches": ["http://www.readability.com/*"],
      "css": ["solarized_light.css", "styles.css"],
      "js": ["jquery.js", "highlight.pack.js", "script.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
