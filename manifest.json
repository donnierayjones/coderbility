{
  "name": "Coderbility",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Add syntax highlighting to Readability & Instapaper",
  "options_page": "options.html",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "options.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.instapaper.com/*",
        "*://www.readability.com/*",
        "*://readability.com/*"
      ],
      "js": ["jquery.js", "highlight.pack.js", "script.js"]
    }
  ],
  "web_accessible_resources": [
    "styles/default.css",
    "styles/agate.css",
    "styles/androidstudio.css",
    "styles/arta.css",
    "styles/ascetic.css",
    "styles/atelier-cave.dark.css",
    "styles/atelier-cave.light.css",
    "styles/atelier-dune.dark.css",
    "styles/atelier-dune.light.css",
    "styles/atelier-estuary.dark.css",
    "styles/atelier-estuary.light.css",
    "styles/atelier-forest.dark.css",
    "styles/atelier-forest.light.css",
    "styles/atelier-heath.dark.css",
    "styles/atelier-heath.light.css",
